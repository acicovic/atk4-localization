{
  "repositories": [
    {
      "type": "path",
      "url": "atk4/ui",
      "replace": {
        "atk4/ui": "dev-develop"
      },
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "atk4/login",
      "replace": {
        "atk4/login": "dev-develop"
      },
      "options": {
        "symlink": true
      }
    }
  ],
  "require": {
    "atk4/ui": "dev-develop",
    "atk4/login": "dev-develop",
    "atk4/data": "dev-develop",
    "atk4/dsql": "dev-develop",
    "atk4/core": "dev-develop"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  }
}